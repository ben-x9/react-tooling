{
  "name": "react-tooling",
  "version": "0.25.2",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "description": "Tooling for React",
  "repository": "github.com/ben-x9/react-tooling",
  "authors": [
    "Ben Lambert",
    "Jalal Chaabane"
  ],
  "license": "MIT",
  "keywords": [
    "react",
    "reactjs",
    "redux",
    "typescript",
    "elm-architecture"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc",
    "prepare": "npm run build",
    "compile": "tsc",
    "format": "prettier --write \"**/*.{ts*(x),js*(on),scss}\"",
    "check:format": "prettier -l \"**/*.{ts*(x),js*(on),scss}\"",
    "check": "npm run compile && npm run check:format"
  },
  "dependencies": {
    "functools-ts": "^0.0.15",
    "react": "^16.6.1",
    "react-hot-loader": "^4.3.12",
    "react-redux": "^5.1.0",
    "redux": "^4.0.1",
    "remote-redux-devtools": "^0.5.13",
    "rxjs": "^6.3.3"
  },
  "devDependencies": {
    "@types/history": "^4.6.2",
    "@types/node": "^8.0.54",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-hot-loader": "^3.0.5",
    "@types/react-redux": "^6.0.7",
    "@types/redux": "^3.6.0",
    "@types/redux-devtools-extension": "^2.13.2",
    "@types/remote-redux-devtools": "^0.5.2",
    "prettier": "^1.14.3",
    "tslint": "^5.8.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "^3.0.3"
  }
}
